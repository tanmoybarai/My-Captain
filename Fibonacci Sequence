n = int(input("How many terms? "))

a = 0
b = 1
count = 0

if n <= 0:
   print("Please enter a positive integer")
elif n == 1:
   print("Fibonacci sequence upto")
   print(n1)
else:
   print("Fibonacci sequence:")
   while count < n:
       print(a)
       x = a + b
       
       a = b
       b = x
       count += 1
